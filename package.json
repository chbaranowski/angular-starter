{
  "name": "angular-starter",
  "version": "1.0.0",
  "description": "A super simple Angular2 example.",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --inline --port 8080",
    "start:local": "cross-env BUILD_MODE=debug cross-env CONFIG_PROFILE=local npm run start",
    "start:prod": "cross-env BUILD_MODE=production cross-env CONFIG_PROFILE=prod npm run start",
    "build": "rimraf dist && webpack --progress --profile --bail",
    "build:local": "cross-env BUILD_MODE=debug cross-env CONFIG_PROFILE=local npm run build",
    "build:prod": "cross-env BUILD_MODE=production cross-env CONFIG_PROFILE=prod npm run build",
    "docs": "rimraf documentation && ./node_modules/.bin/compodoc -p tsconfig.json",
    "i18n": "ng-xi18n"
  },
  "author": "Christian Baranowski",
  "license": "ISC",
  "devDependencies": {
    "@angular/compiler-cli": "^4.0.0",
    "@types/node": "^7.0.8",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.1.2",
    "bootstrap-sass": "^3.3.7",
    "compodoc": "0.0.41",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.2.4",
    "css-loader": "^0.27.3",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.4",
    "node-sass": "^4.5.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.0",
    "stylelint": "^7.9.0",
    "tslint": "^4.5.1",
    "tslint-loader": "^3.4.3",
    "typescript": "^2.2.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.2",
    "stylelint-config-suitcss": "^10.0.0",
    "stylelint-webpack-plugin": "^0.7.0"
  },
  "dependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/platform-server": "^4.0.0",
    "@angular/router": "^4.0.0",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.2.0",
    "zone.js": "^0.8.5"
  }
}
