{
  "name": "angular-starter",
  "version": "1.0.0",
  "description": "A super simple Angular2 example.",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --inline --port 8080",
    "start:local": "CONFIG_PROFILE=local npm run start",
    "start:prod": "CONFIG_PROFILE=prod npm run start",
    "build": "rimraf dist && webpack --progress --profile --bail",
    "build:local": "CONFIG_PROFILE=local npm run build",
    "build:prod": "BUILD_MODE=production CONFIG_PROFILE=prod npm run build",
    "docs": "rimraf documentation && ./node_modules/.bin/compodoc -p tsconfig.json"
  },
  "author": "Christian Baranowski",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^6.0.52",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "^3.0.0-beta.17",
    "bootstrap-sass": "^3.3.7",
    "compodoc": "0.0.32",
    "css-loader": "^0.25.0",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.26.0",
    "json-loader": "^0.5.4",
    "node-sass": "^4.1.1",
    "postcss-loader": "^1.2.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "typescript": "^2.1.4",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "@angular/common": "^2.4.1",
    "@angular/compiler": "^2.4.1",
    "@angular/core": "^2.4.1",
    "@angular/forms": "^2.4.1",
    "@angular/platform-browser": "^2.4.1",
    "@angular/platform-browser-dynamic": "^2.4.1",
    "@angular/router": "^3.4.1",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.9",
    "rxjs": "^5.0.2",
    "zone.js": "^0.7.4"
  }
}
